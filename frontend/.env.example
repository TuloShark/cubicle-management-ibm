# Frontend Environment Configuration
# Copy this file to .env and fill in your actual values

# Firebase Configuration (get these from your Firebase project settings)
VITE_API_KEY=your_firebase_api_key
VITE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_PROJECT_ID=your-project-id
VITE_STORAGE_BUCKET=your-project.appspot.com
VITE_MESSAGING_SENDER_ID=123456789
VITE_APP_ID=1:123456789:web:abcdef123456
VITE_MEASUREMENT_ID=G-XXXXXXXXXX

# Admin User IDs (must match backend configuration)
VITE_ADMIN_UIDS=your_firebase_uid_1,your_firebase_uid_2

# Optional: Sentry Configuration (leave empty to disable)
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project

# API Configuration
VITE_API_BASE_URL=http://localhost:3000
